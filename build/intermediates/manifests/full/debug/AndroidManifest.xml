<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="development.assistant.project.s_assistant"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="26" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="development.assistant.project.s_assistant.AppBase" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="development.assistant.project.s_assistant.profile_activity"
            android:parentActivityName="development.assistant.project.s_assistant.AppBase" />
        <activity
            android:name="development.assistant.project.s_assistant.Student_Registartion"
            android:parentActivityName="development.assistant.project.s_assistant.profile_activity" />
        <activity
            android:name="development.assistant.project.s_assistant.attendanceActivity"
            android:parentActivityName="development.assistant.project.s_assistant.AppBase" />
        <activity
            android:name="development.assistant.project.s_assistant.scheduler"
            android:parentActivityName="development.assistant.project.s_assistant.AppBase" />
        <activity
            android:name="development.assistant.project.s_assistant.make_schedule"
            android:parentActivityName="development.assistant.project.s_assistant.scheduler" />
        <activity
            android:name="development.assistant.project.s_assistant.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="development.assistant.project.s_assistant.AppBase" />
        <activity
            android:name="development.assistant.project.s_assistant.About"
            android:parentActivityName="development.assistant.project.s_assistant.AppBase" />
        <activity
            android:name="development.assistant.project.s_assistant.Edit_Student"
            android:parentActivityName="development.assistant.project.s_assistant.AppBase" />
    </application>

</manifest>